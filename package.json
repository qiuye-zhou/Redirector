    "@unocss/webpack": "^66.1.1",
    "antd": "^5.25.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-import": "^1.13.8",
    "copy-webpack-plugin": "^13.0.0",
    "core-js": "^3.44.0",
    "css-loader": "^6.11.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
